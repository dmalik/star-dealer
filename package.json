{
  "name": "star-events",
  "version": "2.0.0",
  "description": "Front-end Take-home Challenge",
  "main": "app.js",
  "scripts": {
    "addevents": "node scripts/addTestEvents.js",
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "prod": "set NODE_ENV=production&& npm run build",
    "postinstall": "npm run build",
    "lint": "eslint --fix **/*.{js,jsx}",
    "start": "node app.js"
  },
  "vitreum": {
    "assets": [
      "*.svg",
      "*.png",
      "*.jpg",
      "*.otf",
      "*.woff",
      "*.ico",
      "*.ttf",
      "*.js"
    ],
    "bundled": [
      "react",
      "react-dom",
      "create-react-class",
      "lodash/core",
      "classnames",
      "superagent"
    ],
    "shared": [
      "./client"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "babel": {
    "only": [
      "*.js",
      "*.jsx"
    ],
    "presets": [
      "stage-3",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+URL"
  },
  "author": "Thalmic Labs - DM",
  "license": "ISC",
  "bugs": {
    "url": "issues link"
  },
  "homepage": "home link",
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.2",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.0",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "nconf": "^0.9.1",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-modal": "^3.1.13",
    "superagent": "^3.8.2",
    "vitreum": "^4.10.1"
  }
}
